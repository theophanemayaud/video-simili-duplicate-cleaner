{
    "cmake.sourceDirectory": "${workspaceFolder}/QtProject",
    "cmake.copyCompileCommands": "${workspaceFolder}/compile_commands.json",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.ctestArgs": [
        "-V",
        "--output-on-failure"
    ],
    "cmake.ctest.allowParallelJobs": true,
    "cmake.debugConfig": {
        "name": "Debug (lldb)",
        "type": "cppdbg",
        "request": "launch",
        "program": "${command:cmake.launchTargetPath}",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${command:cmake.launchTargetDirectory}",
        "environment": [],
        "MIMode": "lldb",
        "externalConsole": false,
    },
    "files.associations": {},
    "testMate.cpp.test.executables": "${workspaceFolder}/QtProject/builds/**/*{test,Test,TEST}*",
    "testMate.cpp.test.workingDirectory": "${workspaceFolder}/QtProject",
    "testMate.cpp.log.logpanel": true
}