{
    "version": 3,
    "configurePresets": [
        {
            "name": "base-6.10.1-macos",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_PREFIX_PATH": "${sourceDir}/libraries/macos/qt/qt-install",
                "CMAKE_OSX_ARCHITECTURES": "x86_64;arm64"
            },
            "inherits": [
                "base-ninja",
                "base-macos"
            ],
            "environment": {
                "PATH": "${sourceDir}/libraries/macos/qt/qt-install/bin:$env{HOME}/Qt/Tools/Ninja/:$penv{PATH}"
            }
        },
        {
            "name": "debug-6.10.1-macos",
            "binaryDir": "${sourceDir}/builds/build-debug-6.10.1-macos",
            "inherits": [
                "base-6.10.1-macos",
                "base-debug"
            ]
        },
        {
            "name": "release-macos",
            "binaryDir": "${sourceDir}/release-build/macos/build",
            "inherits": [
                "base-6.10.1-macos",
                "base-release"
            ]
        }
    ]
}